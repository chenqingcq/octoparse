@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <!--<meta http-equiv="Content-Language" content="en-US" />-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=0, user-scalable=no">
    @RenderSection("metas", required: false)
    <link rel="shortcut icon" href="/images/logo_blue.png">
    <link rel="apple-touch-icon" href="/images/logo_blue.png">
    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/font-awesome.min.css" rel="stylesheet">
    <link href="/css/app.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/flex.css">
    <!--[if lt IE 9]>
    <script src="/scripts/html5shiv.min.js"></script>
    <script src="/scripts/respond.min.js"></script>
    <![endif]-->
    <!-- Start of octoparse Zendesk Widget script -->
    <script id="ze-snippet" src="https://static.zdassets.com/ekr/snippet.js?key=b1cbeaf5-69a2-4e59-8f79-57a36817dd56"> </script>
    <!-- End of octoparse Zendesk Widget script -->
    @RenderSection("styles", required: false)
    <style type="text/css">
        html{
            font-size: 20px !important;
            overflow-x:hidden;
            height: auto;
        }
        .chat a {
            opacity: 0.7;
        }

            .chat a:hover {
                opacity: 1;
            }
    </style>
    <title>octoparse</title>
</head>
<body>
    @{Html.RenderPartial("octHeader"); }
    <div class="content" >
        @RenderBody()
    </div>
    @{Html.RenderPartial("octFooter"); }
    <script src="/scripts/jquery.min.js"></script>
    <script src="/scripts/bootstrap.min.js"></script>
    <script src="/scripts/jquery.cookie.js"></script>
    <script src="/scripts/blackFriday.js"></script>
    <script src="/scripts/app.js"></script>
    @RenderSection("scripts", required: false)
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
      ga('create', 'UA-69472089-3', 'auto');
      ga('send', 'pageview');
    </script>
    <script>
        (function(){
            var timer = null ;
            timer = setInterval(function(){
                var iframe = document.querySelector('#launcher');
                if(iframe){
                    clearInterval(timer);
                    iframe.style.display = 'none'
                }
            },60)
        })()
    </script>
</body>
</html>